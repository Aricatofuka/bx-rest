{
  "name": "bx-rest",
  "version": "0.0.3",
  "description": "bitrix rest api",
  "typings": "./index.d.ts",
  "type": "module",
  "dependencies": {
    "@angular/common": "^16.1.8",
    "@angular/core": "^16.1.8",
    "@angular/material": "^16.1.7",
    "@angular/router": "^16.1.8",
    "@ngrx/store": "^16.2.0",
    "entities": "^4.5.0",
    "just-clone": "^6.2.0",
    "just-compare": "^2.3.0",
    "luxon": "^3.3.0",
    "parse5": "^7.1.2",
    "rxjs": "^7.8.1",
    "safevalues": "^0.3.4",
    "tslib": "^2.6.1",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "just-flatten-it": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aricatofuka/bx-rest.git"
  },
  "scripts": {
    "compile": "tsc -b ./src/tsconfig.cjs.json ./src/tsconfig.cjs.spec.json ./src/tsconfig.esm.json ./src/tsconfig.esm5.json ./src/tsconfig.esm5.rollup.json ./src/tsconfig.types.json ./src/tsconfig.types.spec.json ./spec/tsconfig.json",
    "build:clean": "shx rm -rf ./dist"
  },
  "keywords": [
    "bitrxi",
    "rest",
    "api"
  ],
  "author": "Aricatofuka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aricatofuka/bx-rest/issues"
  },
  "homepage": "https://github.com/Aricatofuka/bx-rest#readme",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/types/index.d.ts",
      "node": "./dist/cjs/index.js",
      "require": "./dist/cjs/index.js",
      "es2015": "./dist/esm/index.js",
      "default": "./dist/esm5/index.js"
    }
  }
}
