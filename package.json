{
    "name": "bx-rest",
    "version": "0.0.4",
    "description": "bitrix rest api",
    "typings": "./index.d.ts",
    "type": "module",
    "dependencies": {
        "@angular-devkit/build-optimizer": "0.4.6",
        "@angular-devkit/schematics": "^11.0.7",
        "@angular/common": "^16.1.8",
        "@angular/core": "^16.1.8",
        "@angular/material": "^16.1.7",
        "@angular/router": "^16.1.8",
        "@ngrx/store": "^16.2.0",
        "@swc/core": "^1.2.128",
        "@swc/helpers": "^0.3.2",
        "@types/chai": "^4.2.11",
        "@types/estree": "^1.0.1",
        "@types/lodash": "4.14.102",
        "@types/mocha": "^7.0.2",
        "@types/node": "^14.14.6",
        "@types/shelljs": "^0.8.8",
        "@types/sinon": "4.1.3",
        "@types/sinon-chai": "2.7.29",
        "@types/source-map": "^0.5.2",
        "@typescript-eslint/eslint-plugin": "^4.29.1",
        "@typescript-eslint/parser": "^4.29.1",
        "babel-polyfill": "6.26.0",
        "chai": "^4.2.0",
        "check-side-effects": "0.0.23",
        "color": "3.0.0",
        "colors": "1.1.2",
        "cross-env": "5.1.3",
        "cz-conventional-changelog": "1.2.0",
        "dependency-cruiser": "^9.12.0",
        "entities": "^4.5.0",
        "escape-string-regexp": "1.0.5",
        "eslint": "^7.8.1",
        "eslint-plugin-jasmine": "^2.10.1",
        "form-data": "^3.0.0",
        "fs-extra": "^11.1.1",
        "glob": "7.1.2",
        "google-closure-compiler-js": "20170218.0.0",
        "husky": "^4.2.5",
        "just-clone": "^6.2.0",
        "just-compare": "^2.3.0",
        "klaw-sync": "3.0.2",
        "lint-staged": "^10.2.11",
        "lodash": "^4.17.15",
        "luxon": "^3.3.0",
        "minimist": "^1.2.5",
        "nodemon": "^1.9.2",
        "npm-run-all": "4.1.2",
        "opn-cli": "3.1.0",
        "parse5": "^7.1.2",
        "platform": "1.3.5",
        "prettier": "^2.5.1",
        "promise": "8.0.1",
        "require": "^2.4.20",
        "requirejs": "^2.3.6",
        "rollup": "0.66.6",
        "rollup-plugin-alias": "1.4.0",
        "rollup-plugin-inject": "2.0.0",
        "rollup-plugin-node-resolve": "2.0.0",
        "rxjs": "^7.8.1",
        "safevalues": "^0.3.4",
        "shelljs": "^0.8.4",
        "shx": "^0.3.2",
        "sinon": "4.3.0",
        "sinon-chai": "2.14.0",
        "source-map-support": "0.5.3",
        "systemjs": "^0.21.0",
        "ts-node": "^9.1.1",
        "tslib": "^2.6.1",
        "tslint": "^5.20.1",
        "tslint-config-prettier": "^1.18.0",
        "tslint-etc": "1.13.10",
        "tslint-no-toplevel-property-access": "0.0.2",
        "tslint-no-unused-expression-chai": "0.0.3",
        "validate-commit-msg": "2.14.0",
        "web-streams-polyfill": "^3.0.2",
        "webpack": "^5.88.2",
        "zone.js": "^0.13.1"
    },
    "devDependencies": {
        "@types/luxon": "^3.3.1",
        "just-flatten-it": "^5.2.0",
        "typescript": "^4.9.5"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Aricatofuka/bx-rest.git"
    },
    "scripts": {
        "lint_spec": "tslint -c spec/tslint.json -p spec/tsconfig.json \"spec/**/*.ts\"",
        "lint_src": "tslint -c tslint.json -p src/tsconfig.base.json \"src/**/*.ts\"",
        "lint": "npm-run-all --parallel lint_*",
        "dtslint": "tsc -b ./src/tsconfig.types.json && tslint -c spec-dtslint/tslint.json -p spec-dtslint/tsconfig.json \"spec-dtslint/**/*.ts\"",
        "prepublishOnly": "npm run build:package && npm run lint && npm run test && npm run test:circular && npm run dtslint",
        "compile": "tsc -b ./src/tsconfig.cjs.json ./src/tsconfig.cjs.spec.json ./src/tsconfig.esm.json ./src/tsconfig.esm5.json ./src/tsconfig.esm5.rollup.json ./src/tsconfig.types.json ./src/tsconfig.types.spec.json ./spec/tsconfig.json",
        "build:clean": "shx rm -rf ./dist",
        "build:global": "node ./tools/make-umd-bundle.js && node ./tools/make-closure-core.js",
        "build:package": "npm-run-all build:clean compile",
        "watch:dtslint": "nodemon -w \"src/\" -w \"spec-dtslint/\" -e ts -x npm run dtslint"
    },
    "//": [
        "build:package",
        "npm-run-all build:clean compile && node ./tools/generate-alias.js"
    ],
    "keywords": [
        "bitrxi",
        "rest",
        "api"
    ],
    "author": "Aricatofuka",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Aricatofuka/bx-rest/issues"
    },
    "homepage": "https://github.com/Aricatofuka/bx-rest#readme",
    "exports": {
        "./package.json": {
            "default": "./package.json"
        },
        ".": {
            "types": "./dist/types/index.d.ts",
            "node": "./dist/cjs/index.js",
            "require": "./dist/cjs/index.js",
            "es2015": "./dist/esm/index.js",
            "default": "./dist/esm5/index.js"
        }
    }
}
